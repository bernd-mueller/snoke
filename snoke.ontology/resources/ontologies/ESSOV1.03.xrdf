    <!-- http://www.semanticweb.org/rjyy/ontologies/2015/5/ESSO#ILAE_1981_2.1.1.4_Typical_Absence_Seizures_With_Tonic_Components -->

    <Class rdf:about="http://www.semanticweb.org/rjyy/ontologies/2015/5/ESSO#ILAE_1981_2.1.1.4_Typical_Absence_Seizures_With_Tonic_Components">
