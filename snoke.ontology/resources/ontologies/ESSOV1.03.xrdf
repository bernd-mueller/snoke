    <!-- http://www.semanticweb.org/rjyy/ontologies/2015/5/ESSO#ILAE_1981_4.4.3_Epilepsia_Partialis_Continua -->

    <Class rdf:about="http://www.semanticweb.org/rjyy/ontologies/2015/5/ESSO#ILAE_1981_4.4.3_Epilepsia_Partialis_Continua">
