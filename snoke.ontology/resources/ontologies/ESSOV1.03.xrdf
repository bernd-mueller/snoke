        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rjyy/ontologies/2015/5/ESSO#Benign_Epileptiform"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rjyy/ontologies/2015/5/ESSO#Noachter_1999_Benign_Epileptiform_Transient_of_Sleep_BETS"/>
