        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rjyy/ontologies/2015/5/ESSO#ILAE_2010_1.3_Childhood"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rjyy/ontologies/2015/5/ESSO#Idiopathic_Focal_Epilepsy_of_Infancy_and_Childhood"/>
