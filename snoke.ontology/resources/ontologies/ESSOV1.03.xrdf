    <!-- http://www.semanticweb.org/rjyy/ontologies/2015/5/ESSO#Sialidosis_with_Isolated_Deficit_in_Neuroamidinase -->

    <Class rdf:about="http://www.semanticweb.org/rjyy/ontologies/2015/5/ESSO#Sialidosis_with_Isolated_Deficit_in_Neuroamidinase">
