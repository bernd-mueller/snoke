<?xml version="1.0" encoding="UTF-8"?>
<cpeDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <collectionReader>
        <collectionIterator>
            <descriptor>        
                <import location="descriptors/zbmed/uima/reader/BioASQMongoDBReader.xml"/>        
            </descriptor>  
            <configurationParameterSettings>       
                <nameValuePair>           
                    <name>SrcField</name>         
                    <value>                  
                        <string>title@abstractText</string>
                    </value>
                                    
                </nameValuePair>
                                
                <nameValuePair>
                                        
                    <name>Language</name>
                                        
                    <value>
                                                
                        <string/>
                                            
                    </value>
                                    
                </nameValuePair>
                                
                <nameValuePair>
                                        
                    <name>MongoServer</name>
                                        
                    <value>
                                                
                        <string>localhost</string>
                                            
                    </value>
                                    
                </nameValuePair>
                                
                <nameValuePair>
                                        
                    <name>MongoDB</name>
                                        
                    <value>
                                                
                        <string>snoke</string>
                                            
                    </value>
                                    
                </nameValuePair>
                                
                <nameValuePair>
                                        
                    <name>MongoCollection</name>
                                        
                    <value>
                                                
                        <string>snokeall</string>
                                            
                    </value>
                                    
                </nameValuePair>
                                
                <nameValuePair>
                                        
                    <name>MaxDoc</name>
                                        
                    <value>
                                                
                        <string>15000000</string>
                                            
                    </value>
                                    
                </nameValuePair>
                
                <nameValuePair>
                                        
                    <name>StartYear</name>
                                        
                    <value>
                                                
                        <string>0</string>
                                            
                    </value>
                                    
                </nameValuePair>
                            
            </configurationParameterSettings>
                    
        </collectionIterator>
            
    </collectionReader>
    <casProcessors casPoolSize="256" processingUnitThreadCount="64">
            
        <casProcessor deployment="integrated" name="TokenizerZBMED">
                    
            <descriptor>
                            
                <import location="descriptors/zbmed/uima/analysis/TokenizerZBMED.xml"/>
                        
            </descriptor>
	    <deploymentParameters/>
	    <configurationParameterSettings>
                            
                  <nameValuePair>
					                                      
                     <name>Stemmer</name>
							                                         
                         <value>
											                                            
                        <string>de.zbmed.snoke.uima.analysis.ZBMEDSnowBallStemmer</string>
													                                               
                    </value>
													                                    
                </nameValuePair>
														                                
													                            
            </configurationParameterSettings>				
            <errorHandling>
                            
                <errorRateThreshold action="terminate" value="0/1000"/>
                            
                <maxConsecutiveRestarts action="terminate" value="30"/>
                            
                <timeout max="100000" default="-1"/>
                        
            </errorHandling>
                    
            <checkpoint batch="10000" time="1000ms"/>
                
        </casProcessor>
         
        <casProcessor deployment="integrated" name="ConceptMapper_Dict_MeSH">
                    
            <descriptor>
                            
                <import location="descriptors/zbmed/uima/analysis/ConceptMapperOffsetTokenizer_Dict_MeSH.xml"/>
                        
            </descriptor>
            <deploymentParameters/>
                    
            <errorHandling>
                            
                <errorRateThreshold action="terminate" value="0/1000"/>
                            
                <maxConsecutiveRestarts action="terminate" value="30"/>
                            
                <timeout max="100000" default="-1"/>
                        
            </errorHandling>
                    
            <checkpoint batch="10000" time="1000ms"/>
                    
            <configurationParameterSettings>
                            
                <nameValuePair>
                                    
                    <name>FindAllMatches</name>
                                    
                    <value>
                                            
                        <boolean>true</boolean>
                                        
                    </value>
                                
                </nameValuePair>
                        
            </configurationParameterSettings>
                
        </casProcessor>
            
        <casProcessor deployment="integrated" name="ConceptMapper_Dict_EpSO">
                    
            <descriptor>
                            
                <import location="descriptors/zbmed/uima/analysis/ConceptMapperOffsetTokenizer_Dict_EpSO.xml"/>
                        
            </descriptor>
            <deploymentParameters/>
                    
            <errorHandling>
                            
                <errorRateThreshold action="terminate" value="0/1000"/>
                            
                <maxConsecutiveRestarts action="terminate" value="30"/>
                            
                <timeout max="100000" default="-1"/>
                        
            </errorHandling>
                    
