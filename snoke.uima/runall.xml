            </configurationParameterSettings>
                
        </casProcessor>
            
        <casProcessor deployment="integrated" name="MongoDB Writer CAS Consumer">
                    
            <descriptor>
                            
                <import location="descriptors/zbmed/uima/consumer/ConsumerMongoDB.xml"/>
                        
            </descriptor>
            <deploymentParameters/>
                    
            <errorHandling>
                            
                <errorRateThreshold action="terminate" value="0/1000"/>
                            
                <maxConsecutiveRestarts action="terminate" value="30"/>
                            
                <timeout max="100000" default="-1"/>
                        
            </errorHandling>
                    
            <checkpoint batch="10000" time="1000ms"/>
                    
            <configurationParameterSettings>
                            
                <nameValuePair>
                                    
                    <name>MongoServer</name>
                                    
                    <value>
                                            
                        <string>localhost</string>
                                        
                    </value>
                                
                </nameValuePair>
                            
                <nameValuePair>
                                    
                    <name>MongoDB</name>
                                    
                    <value>
                                            
                        <string>uima</string>
                                        
                    </value>
                                
                </nameValuePair>
                            
                <nameValuePair>
                                    
                    <name>MongoCollection</name>
                                    
                    <value>
                                            
                        <string>uimaall</string>
                                        
                    </value>
                                
                </nameValuePair>
                        
            </configurationParameterSettings>
                
        </casProcessor>
    </casProcessors>
        
    <cpeConfig>
                
        <numToProcess>-1</numToProcess>
                
        <deployAs>immediate</deployAs>
                
        <checkpoint batch="1" time="300000ms"/>
                
        <timerImpl/>
            
    </cpeConfig>
    
    
</cpeDescription>
