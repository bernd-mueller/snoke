            <configurationParameterSettings>
                            
                <nameValuePair>
                                    
                    <name>FindAllMatches</name>
                                    
                    <value>
                                            
                        <boolean>true</boolean>
                                        
                    </value>
                                
                </nameValuePair>
                        
            </configurationParameterSettings>
                
        </casProcessor>
            
          <casProcessor deployment="integrated" name="ConceptMapper_Dict_ESSO">
                    
